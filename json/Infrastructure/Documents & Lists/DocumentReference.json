  "created<dateTime>$(Document creation time$)": null,
  "indexed<instant>$(R!  When this document reference was created$)": null,
  "author<list::Reference(Practitioner|Organization|Device|Patient|RelatedPerson)>$(Who and/or what authored the document$)": null,
  "authenticator<Reference(Practitioner|Organization)>$(Who/what authenticated the document$)": null,
